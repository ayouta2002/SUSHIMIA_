<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="listReservationPane" prefHeight="602.0" prefWidth="790.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.addReservationController">
    <stylesheets>
        <URL value="@hello-view.css" />
    </stylesheets>
    <children>
        <Label layoutX="181.0" layoutY="250.0" prefHeight="18.0" prefWidth="86.0" styleClass="label-ajout" stylesheets="@hello-view.css" text="Description :" />
        <Label layoutX="181.0" layoutY="312.0" prefHeight="18.0" prefWidth="98.0" styleClass="label-ajout" stylesheets="@hello-view.css" text="Evenement :" />
        <Button fx:id="btnAddReservation" layoutX="430.0" layoutY="499.0" mnemonicParsing="false" onAction="#AjoutReservation" prefHeight="34.0" prefWidth="66.0" styleClass="btn" stylesheets="@hello-view.css" text="Add" />
        <Button fx:id="btnClearReservation" layoutX="338.0" layoutY="500.0" mnemonicParsing="false" onAction="#clearFieldsReservation" prefHeight="34.0" prefWidth="77.0" styleClass="btn" stylesheets="@hello-view.css" text="Clear" />
        <Button fx:id="btnReturnReservation" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#return_ListReservation" prefHeight="34.0" prefWidth="66.0" styleClass="btn" stylesheets="@hello-view.css">
         <graphic>
            <ImageView fitHeight="20.0" fitWidth="26.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/return.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <TextArea fx:id="txtDescriptionRes" layoutX="304.0" layoutY="241.0" prefHeight="52.0" prefWidth="175.0" promptText="Description" styleClass="textarea" stylesheets="@hello-view.css" />
      <ComboBox fx:id="txtEventRes" layoutX="304.0" layoutY="309.0" prefHeight="26.0" prefWidth="175.0" promptText="Choisir Evenement" styleClass="comboBox" stylesheets="@hello-view.css" />
      <Label layoutX="284.0" layoutY="112.0" prefHeight="34.0" prefWidth="214.0" text="Ajouter une Reservation" textFill="#dfd401">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Label>

    </children>

</AnchorPane>
